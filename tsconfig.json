{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve",
    "resolveJsonModule": true
  },
  "paths": {
    "prisma": [
      "./projects/lib/prisma.ts"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/api/login.js",
    "pages/api/contacts.js",
    "pages/api/contacts.js",
    "pages/_app.jsx",
    "pages/index.tsx",
    "pages/api/contacts.js"
, "lib/prisma.js", "pages/api/create.js"  ],
  "exclude": [
    "node_modules"
  ]
}
