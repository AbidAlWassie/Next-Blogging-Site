{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "paths": {
    "my-lib": [
      "projects/lib/src/index.ts"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/api/login.js", "pages/api/contacts.js", "pages/api/contacts.js", "pages/_app.jsx", "pages/index.jsx", "pages/api/contacts.js"  ],
  "exclude": [
    "node_modules"
  ]
}
